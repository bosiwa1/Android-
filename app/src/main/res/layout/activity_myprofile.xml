<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#11EFECEC"
    android:backgroundTint="#E1E3E1"
    tools:context=".Myprofile">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">


        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent">


            <LinearLayout
                android:layout_width="411dp"
                android:layout_height="45dp"
                android:layout_marginTop="4dp"
                android:background="#F4F3F3"
                android:orientation="horizontal"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.0"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/linearLayout4">

                <ImageButton
                    android:id="@+id/imageButton"
                    android:layout_width="36dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:background="#A4FFFFFF"
                    app:srcCompat="@drawable/grid"
                    android:contentDescription="@string/k" />

                <ImageButton
                    android:id="@+id/imageButton2"
                    android:layout_width="40dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:background="#7AFFFFFF"
                    app:srcCompat="@drawable/video"
                    android:contentDescription="@string/k1" />

                <ImageButton
                    android:id="@+id/imageButton3"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:background="#70FFFFFF"
                    app:srcCompat="@drawable/personalyse"
                    android:contentDescription="@string/k3" />
            </LinearLayout>

            <LinearLayout
                android:id="@+id/linearLayout4"
                android:layout_width="408dp"
                android:layout_height="46dp"
                android:layout_marginTop="8dp"
                android:layout_marginEnd="2dp"
                android:layout_marginBottom="50dp"
                android:background="#BAFFFFFF"
                android:orientation="horizontal"
                app:layout_constraintBottom_toTopOf="@+id/linearLayout"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.0"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/linearLayout3"
                app:layout_constraintVertical_bias="1.0">

                <Button
                    android:id="@+id/button"
                    style="?android:attr/buttonBarButtonStyle"
                    android:layout_width="204dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:background="#FFFFFF"
                    android:backgroundTintMode="add"
                    android:text="@string/edit_profile"
                    android:textColor="@color/black"
                    android:textSize="16sp"
                    android:textStyle="bold" />

                <Button
                    android:id="@+id/button2"
                    style="?android:attr/buttonBarButtonStyle"

                    android:layout_width="190dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1"

                    android:background="#FFFFFF"
                    android:text="@string/share_profile"
                    android:textColor="@color/black"
                    android:textSize="16sp"
                    android:textStyle="bold" />
            </LinearLayout>

            <LinearLayout
                android:id="@+id/linearLayout3"
                android:layout_width="408dp"
                android:layout_height="126dp"
                android:layout_marginStart="16dp"
                android:layout_marginTop="24dp"
                android:layout_marginBottom="110dp"
                android:orientation="vertical"
                app:layout_constraintBottom_toTopOf="@+id/linearLayout"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/linearLayout2">

                <TextView
                    android:id="@+id/textView"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="John Conor "
                    android:textSize="16sp"
                    android:textStyle="bold" />

                <TextView
                    android:id="@+id/textView2"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="Gratuated at University of Limerick in Software Developement"
                    android:textColor="#4485BE"
                    android:textSize="16sp"
                    android:textStyle="bold" />

                <TextView
                    android:id="@+id/textView3"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="Work at work at Del as back from developer StackHolder"
                    android:textColor="#3F82B5"
                    android:textSize="16sp"
                    android:textStyle="bold" />

                <TextView
                    android:id="@+id/textView4"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="youtube.com/channel/UCBrjhQvveDz_HY4XVdfm4-g"
                    android:textStyle="bold" />

                <TextView
                    android:id="@+id/textView5"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="Live at limerick city .Ireland "
                    android:textColor="#3F82B5"
                    android:textSize="16sp"
                    android:textStyle="bold" />
            </LinearLayout>

            <LinearLayout
                android:id="@+id/linearLayout2"
                android:layout_width="409dp"
                android:layout_height="107dp"
                android:layout_marginTop="24dp"
                android:layout_marginBottom="221dp"
                android:orientation="horizontal"
                app:layout_constraintBottom_toTopOf="@+id/linearLayout"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent">

                <androidx.constraintlayout.widget.ConstraintLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:background="#03FFFFFF">


                    <com.google.android.material.imageview.ShapeableImageView

                        android:id="@+id/shapeableImageView2"
                        android:layout_width="97dp"
                        android:layout_height="match_parent"
                        android:layout_marginTop="8dp"
                        android:layout_marginEnd="0.50dp"
                        android:gravity="center"

                        android:padding="5dp"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintHorizontal_bias="0.0"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toTopOf="parent"
                        app:layout_constraintVertical_bias="0.315"
                        app:shapeAppearanceOverlay="@style/circular"
                        app:srcCompat="@drawable/imag1"
                        app:strokeColor="#833AB4"
                        app:strokeWidth="5dp"
                        tools:ignore="TooDeepLayout" />

                    <com.google.android.material.floatingactionbutton.FloatingActionButton
                        android:id="@+id/floatingActionButton3"
                        android:layout_width="56dp"
                        android:layout_height="55dp"
                        android:layout_marginStart="40dp"
                        android:layout_marginTop="32dp"
                        android:clickable="true"
                        app:backgroundTint="#00FFFFFF"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toTopOf="parent"
                        app:layout_constraintVertical_bias="1.0"
                        app:srcCompat="@drawable/add"
                        android:contentDescription="@string/todof" />

                    <LinearLayout
                        android:layout_width="305dp"
                        android:layout_height="98dp"
                        android:orientation="horizontal"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintStart_toEndOf="@+id/shapeableImageView2">

                        <androidx.constraintlayout.widget.ConstraintLayout
                            android:layout_width="match_parent"
                            android:layout_height="match_parent">

                            <TextView
                                android:id="@+id/textView17"
                                android:layout_width="42dp"
                                android:layout_height="20dp"
                                android:text="@string/post"
                                android:textColor="#040303"
                                android:textStyle="bold"
                                app:layout_constraintBottom_toBottomOf="parent"
                                app:layout_constraintEnd_toEndOf="parent"
                                app:layout_constraintHorizontal_bias="0.23"
                                app:layout_constraintStart_toStartOf="parent"
                                app:layout_constraintTop_toTopOf="parent"
                                app:layout_constraintVertical_bias="0.506" />

                            <TextView
                                android:id="@+id/textView18"
                                android:layout_width="62dp"
                                android:layout_height="20dp"
                                android:shadowColor="#131111"
                                android:text="@string/follower"
                                android:textStyle="bold"
                                app:layout_constraintBottom_toBottomOf="parent"
                                app:layout_constraintEnd_toEndOf="parent"
                                app:layout_constraintHorizontal_bias="0.57"
                                app:layout_constraintStart_toStartOf="parent"
                                app:layout_constraintTop_toTopOf="parent"
                                app:layout_constraintVertical_bias="0.506" />

                            <TextView
                                android:id="@+id/textView19"
                                android:layout_width="66dp"
                                android:layout_height="21dp"
                                android:shadowColor="#121111"
                                android:text="@string/following"
                                android:textColor="#100F0F"
                                android:textStyle="bold"
                                app:layout_constraintBottom_toBottomOf="parent"
                                app:layout_constraintEnd_toEndOf="parent"
                                app:layout_constraintHorizontal_bias="0.935"
                                app:layout_constraintStart_toStartOf="parent"
                                app:layout_constraintTop_toTopOf="parent"
                                app:layout_constraintVertical_bias="0.506" />

                            <TextView
                                android:id="@+id/textView20"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="@string/_234"
                                android:textStyle="bold"
                                app:layout_constraintBottom_toBottomOf="parent"
                                app:layout_constraintEnd_toEndOf="parent"
                                app:layout_constraintHorizontal_bias="0.217"
                                app:layout_constraintStart_toStartOf="parent"
                                app:layout_constraintTop_toTopOf="parent"
                                app:layout_constraintVertical_bias="0.265" />

                            <TextView
                                android:id="@+id/textView21"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="@string/_600"
                                android:textStyle="bold"
                                app:layout_constraintBottom_toBottomOf="parent"
                                app:layout_constraintEnd_toEndOf="@+id/textView19"
                                app:layout_constraintHorizontal_bias="0.526"
                                app:layout_constraintStart_toStartOf="parent"
                                app:layout_constraintTop_toTopOf="parent"
                                app:layout_constraintVertical_bias="0.253" />

                            <TextView
                                android:id="@+id/textView22"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="300"
                                android:textStyle="bold"
                                app:layout_constraintBottom_toBottomOf="parent"
                                app:layout_constraintEnd_toEndOf="parent"
                                app:layout_constraintHorizontal_bias="0.797"
                                app:layout_constraintStart_toStartOf="parent"
                                app:layout_constraintTop_toTopOf="parent"
                                app:layout_constraintVertical_bias="0.253" />
                        </androidx.constraintlayout.widget.ConstraintLayout>
                    </LinearLayout>

                </androidx.constraintlayout.widget.ConstraintLayout>
            </LinearLayout>

            <LinearLayout
                android:id="@+id/linearLayout"
                android:layout_width="414dp"
                android:layout_height="313dp"
                android:layout_marginTop="360dp"
                android:layout_marginBottom="10dp"
                android:orientation="vertical"

                app:layout_constraintBottom_toTopOf="@+id/botton_navigation"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent">

                <ScrollView
                    android:layout_width="match_parent"
                    android:layout_height="311dp"
                    android:layout_marginStart="0dp"

                    android:layout_marginBottom="10dp"
                    android:padding="5dp"
                    tools:ignore="UselessParent">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="5dp"
                        android:orientation="vertical"
                        android:padding="5dp">

                        <LinearLayout
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:orientation="horizontal">

                            <ImageView
                                android:id="@+id/imageView30"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginStart="0dp"
                                android:layout_weight="1"
                                android:background="#1A1919"
                                android:contentDescription="@string/p1"
                                android:padding="5dp"


                                android:scaleType="fitCenter"
                                android:src="@drawable/g4" />

                            <ImageView
                                android:id="@+id/imageView31"
                                android:layout_width="358dp"
                                android:layout_height="match_parent"
                                android:layout_marginStart="0dp"
                                android:layout_weight="1"
                                android:background="#1A1919"
                                android:contentDescription="@string/p2"
                                android:padding="5dp"
                                android:scaleType="fitCenter"
                                android:src="@drawable/g9" />
                        </LinearLayout>

                        <LinearLayout
                            android:layout_width="wrap_content"
                            android:layout_height="match_parent"
                            android:orientation="horizontal">

                            <ImageView
                                android:id="@+id/imageView12"
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:layout_marginStart="0dp"
                                android:layout_weight="1"
                                android:background="#1A1919"
                                android:contentDescription="@string/p4"
                                android:padding="5dp"
                                android:scaleType="fitCenter"
                                android:src="@drawable/ga1" />

                            <ImageView
                                android:id="@+id/imageView13"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginStart="0dp"
                                android:layout_weight="1"
                                android:background="#1A1919"
                                android:contentDescription="@string/p5"
                                android:padding="5dp"
                                android:scaleType="fitCenter"
                                android:src="@drawable/g10" />

                        </LinearLayout>

                        <LinearLayout
                            android:layout_width="wrap_content"
                            android:layout_height="match_parent"
                            android:orientation="horizontal">

                            <ImageView
                                android:id="@+id/imageView15"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginStart="0dp"
                                android:layout_weight="1"
                                android:background="#1A1919"
                                android:contentDescription="@string/p7"
                                android:padding="5dp"
                                android:scaleType="fitCenter"
                                android:src="@drawable/g12" />

                            <ImageView
                                android:id="@+id/imageView17"
                                android:layout_width="wrap_content"
                                android:layout_height="match_parent"
                                android:layout_marginStart="0dp"
                                android:layout_weight="1"
                                android:background="#1A1919"
                                android:contentDescription="@string/p9"
                                android:padding="5dp"
                                android:scaleType="fitCenter"
                                android:src="@drawable/g14" />
                        </LinearLayout>

                        <LinearLayout
                            android:layout_width="wrap_content"
                            android:layout_height="match_parent"
                            android:orientation="horizontal">

                            <ImageView
                                android:id="@+id/imageView21"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginStart="0dp"
                                android:layout_weight="1"
                                android:background="#1A1919"
                                android:contentDescription="@string/p11"
                                android:padding="5dp"
                                android:scaleType="fitCenter"
                                android:src="@drawable/g16" />

                            <ImageView
                                android:id="@+id/imageView22"
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:layout_marginStart="0dp"
                                android:layout_weight="1"
                                android:background="#1A1919"
                                android:contentDescription="@string/p12"
                                android:padding="5dp"
                                android:scaleType="fitCenter"
                                android:src="@drawable/g17" />
                        </LinearLayout>

                        <LinearLayout
                            android:layout_width="wrap_content"
                            android:layout_height="match_parent"
                            android:orientation="horizontal">

                            <ImageView
                                android:id="@+id/imageView24"
                                android:layout_width="wrap_content"
                                android:layout_height="match_parent"
                                android:layout_marginStart="0dp"
                                android:layout_weight="1"
                                android:background="#1A1919"
                                android:contentDescription="@string/p14"
                                android:padding="5dp"
                                android:scaleType="fitCenter"
                                android:src="@drawable/ga11" />

                            <ImageView
                                android:id="@+id/imageView25"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginStart="0dp"
                                android:layout_weight="1"
                                android:background="#1A1919"
                                android:contentDescription="@string/p15"
                                android:padding="5dp"
                                android:scaleType="fitCenter"
                                android:src="@drawable/image7" />

                        </LinearLayout>

                        <LinearLayout
                            android:layout_width="wrap_content"
                            android:layout_height="match_parent"
                            android:orientation="horizontal">

                            <ImageView
                                android:id="@+id/imageView32"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginStart="0dp"
                                android:layout_weight="1"
                                android:background="#1A1919"
                                android:contentDescription="@string/p20"
                                android:padding="5dp"
                                android:scaleType="fitCenter"
                                android:src="@drawable/ga8" />

                            <ImageView
                                android:id="@+id/imageView28"
                                android:layout_width="wrap_content"
                                android:layout_height="match_parent"
                                android:layout_marginStart="0dp"
                                android:layout_weight="1"
                                android:background="#1A1919"
                                android:contentDescription="@string/p18"
                                android:padding="5dp"
                                android:scaleType="fitCenter"
                                android:src="@drawable/g15" />


                        </LinearLayout>
                    </LinearLayout>
                </ScrollView>


            </LinearLayout>

            <LinearLayout
                android:id="@+id/botton_navigation"
                android:layout_width="409dp"
                android:layout_height="68dp"
                android:background="#00FFFFFF"
                android:orientation="horizontal"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent">

                <ImageButton
                    android:id="@+id/back_home"
                    android:layout_width="76dp"
                    android:layout_height="match_parent"
                    android:backgroundTint="#17FFFFFF"
                    android:contentDescription="@string/imagebt1"
                    android:src="@drawable/home"
                    tools:src="@drawable/home" />

                <ImageButton
                    android:id="@+id/search"
                    android:layout_width="80dp"
                    android:layout_height="match_parent"
                    android:backgroundTint="#23FFFFFF"
                    android:contentDescription="@string/imagebt2"
                    android:src="@drawable/search"
                    tools:src="@drawable/search" />

                <ImageButton
                    android:id="@+id/add"
                    android:layout_width="79dp"
                    android:layout_height="match_parent"
                    android:backgroundTint="#27FFFFFF"
                    android:contentDescription="@string/imagebt3"
                    android:src="@drawable/add"
                    tools:src="@drawable/add" />

                <ImageButton
                    android:id="@+id/video"
                    android:layout_width="89dp"
                    android:layout_height="match_parent"
                    android:backgroundTint="#22FFFFFF"
                    android:contentDescription="@string/imagebt4"
                    android:src="@drawable/video"
                    tools:src="@drawable/video" />

                <ImageButton
                    android:id="@+id/profiles1"
                    android:layout_width="106dp"
                    android:layout_height="match_parent"
                    android:backgroundTint="#2AFFFFFF"
                    android:contentDescription="@string/imagebt5"
                    android:src="@drawable/person"
                    tools:src="@drawable/person" />


            </LinearLayout>

        </androidx.constraintlayout.widget.ConstraintLayout>
    </LinearLayout>


</androidx.constraintlayout.widget.ConstraintLayout>